          "org.mongodb:mongodb-driver-core": "4.2.3",
          "org.mongodb:mongodb-driver-sync": "4.2.3",
          "org.jolie-lang:jolie": "10.1.4",
          "org.jolie-lang:libjolie": "10.1.4"
